{
  "rewrites": [
    {
      "source": "/:slug",
      "destination": "/:slug.html"
    }
  ],
  "redirects": [
    {
      "source": "/404/",
      "destination": "/404.html",
      "permanent": false
    },
    {
      "source": "/500/",
      "destination": "/500.html",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    }
  ]
}
